{
    "type": "dark",
    "colors": {
        "editor.background": "#0A0A0A",
        "editor.foreground": "#FFFFFF",

        "editor.selectionBackground": "#F59D004D",
        "focusBorder": "#EB0282",
        "errorForeground": "#F50810"
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#E5A482"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Single Quote Marks",
            "scope": "string.quoted.single.horsescript punctuation.definition",
            "settings": {
                "foreground": "#F59D00"
            }
        },
        {
            "name": "General Punctuation",
            "scope": "punctuation.definition",
            "settings": {
                "foreground": "#D5C2CF"
            }
        },
        {
            "name": "JSON Property Colons",
            "scope": "meta.structure.dictionary.json punctuation.separator.dictionary.key-value",
            "settings": {
                "foreground": "#F59D00"
            }
        },
        {
            "name": "Brackets",
            "scope": "punctuation.section.brackets",
            "settings": {
                "foreground": "#0C89B3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Template Definition",
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#A156D6"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#F36A00"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#F36A00"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#E5A482"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "${foregro}"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#A156D6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#9DC5DE"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "${storage.type::Color}"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#13CC30"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#13CC30"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#13CC30"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#00B1B8"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#A156D6"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#13CC30"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#9DC5DE"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#9DC5DE"
            }
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9DC5DE"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#F50810"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#F50810"
            }
        },
        {
            "name": "JSON Double Quote Marks",
            "scope": "meta.structure.dictionary.json punctuation.support.type.property-name",
            "settings": {
                "foreground": "#AD702A"
            }
        },
        {
            "name": "JSON Property",
            "scope": "meta.structure.dictionary.json support.type.property-name.json",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#D9855B"
            }
        },
        {
            "name": "JSON Property Value",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#EDE8DF"
            }
        },
        {
            "name": "diff.header",
            "scope": "meta.diff, meta.diff.header",
            "settings": {
                "foreground": "#D1B566"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#A156D6"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#13CC30"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "${constantColor}A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#A156D6"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Markup Styling",
            "scope": "markup.bold, markup.italic",
            "settings": {
                "foreground": "#9DC5DE"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#EB0282"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#13CC30"
            }
        },
        {
            "name": "Markup Setext Header",
            "scope": "markup.heading.setext",
            "settings": {
                "foreground": "#13CC30",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Quote",
            "scope": "markup.quote.markdown",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#D1B566"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Link Title/Description",
            "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
            "settings": {
                "foreground": "#F36A00"
            }
        },
        {
            "name": "Markdown Underline Link/Image",
            "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Markdown Emphasis",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Markdown Punctuation Definition Link",
            "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Markdown List Punctuation",
            "scope": [
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#13CC30"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "{$blueLight}"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#F59D00"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#F50810"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#A156D6"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#EB0282"
            }
        }
    ],
    "semanticHighlighting": true
}