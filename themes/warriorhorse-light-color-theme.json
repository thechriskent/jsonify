{
    "type": "dark",
    "colors": {
        "editor.background": "#FFFFFF",
        "editor.foreground": "#323130",

        "editor.selectionBackground": "#B594004D",
        "focusBorder": "#D60987",
        "errorForeground": "#C42700"
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#323130"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#323130"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#99605C"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Single Quote Marks",
            "scope": "string.quoted.single.horsescript punctuation.definition",
            "settings": {
                "foreground": "#B59400"
            }
        },
        {
            "name": "General Punctuation",
            "scope": "punctuation.definition",
            "settings": {
                "foreground": "#605e5c"
            }
        },
        {
            "name": "JSON Property Colons",
            "scope": "meta.structure.dictionary.json punctuation.separator.dictionary.key-value",
            "settings": {
                "foreground": "#B59400"
            }
        },
        {
            "name": "Brackets",
            "scope": "punctuation.section.brackets",
            "settings": {
                "foreground": "#0A7091",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Template Definition",
            "scope": [
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#7E36B3"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": [
                "meta.template.expression"
            ],
            "settings": {
                "foreground": "#323130"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#BF5900"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#BF5900"
            }
        },
        {
            "name": "User-defined constant",
            "scope": "constant.character, constant.other",
            "settings": {
                "foreground": "#99605C"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#323130"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": "",
                "foreground": "${foregro}"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#7E36B3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#1479B8"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "italic",
                "foreground": "${storage.type::Color}"
            }
        },
        {
            "name": "Class name",
            "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#159E42"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#159E42"
            }
        },
        {
            "name": "Function name",
            "scope": "entity.name.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#159E42"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#096685"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#7E36B3"
            }
        },
        {
            "name": "Tag attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "fontStyle": "",
                "foreground": "#159E42"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#1479B8"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#1479B8"
            }
        },
        {
            "name": "Library class/type",
            "scope": "support.type, support.class",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#1479B8"
            }
        },
        {
            "name": "Library variable",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "fontStyle": "",
                "foreground": "#C42700"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#C42700"
            }
        },
        {
            "name": "JSON Double Quote Marks",
            "scope": "meta.structure.dictionary.json punctuation.support.type.property-name",
            "settings": {
                "foreground": "#663F13"
            }
        },
        {
            "name": "JSON Property",
            "scope": "meta.structure.dictionary.json support.type.property-name.json",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#824527"
            }
        },
        {
            "name": "JSON Property Value",
            "scope": "meta.structure.dictionary.json string.quoted.double.json",
            "settings": {
                "foreground": "#323130"
            }
        },
        {
            "name": "diff.header",
            "scope": "meta.diff, meta.diff.header",
            "settings": {
                "foreground": "#D1B566"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#7E36B3"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#159E42"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "${constantColor}A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#7E36B3"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Markup Styling",
            "scope": "markup.bold, markup.italic",
            "settings": {
                "foreground": "#1479B8"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#D60987"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#159E42"
            }
        },
        {
            "name": "Markup Setext Header",
            "scope": "markup.heading.setext",
            "settings": {
                "foreground": "#159E42",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Headings",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Quote",
            "scope": "markup.quote.markdown",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#D1B566"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Link Title/Description",
            "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
            "settings": {
                "foreground": "#BF5900"
            }
        },
        {
            "name": "Markdown Underline Link/Image",
            "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Markdown Emphasis",
            "scope": "markup.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Markdown Punctuation Definition Link",
            "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
            "settings": {
                "foreground": "#323130"
            }
        },
        {
            "name": "Markdown List Punctuation",
            "scope": [
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#159E42"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "{$blueLight}"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#B59400"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#C42700"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#7E36B3"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#D60987"
            }
        }
    ],
    "semanticHighlighting": true
}